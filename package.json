{
  "name": "la-velada-iv-web-oficial",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev  --remote",
    "build": "astro build --remote",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check && tsc --noEmit",
    "lint": "eslint --ext .astro,.ts,.js,.tsx .",
    "lint:fix": "eslint --ext .astro,.ts,.js,.tsx . --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "prepare": "husky install && bash -c 'chmod ug+x .husky/*'"
  },
  "dependencies": {
    "@astrojs/db": "0.14.8",
    "@auth/core": "^0.32.0",
    "@tailwindcss/vite": "^4.0.11",
    "@vercel/analytics": "1.3.1",
    "astro": "5.4.2",
    "auth-astro": "4.1.2",
    "serverless": "link:@astrojs/vercel/serverless",
    "valibot": "0.35.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.43.1",
    "@astrojs/check": "0.9.4",
    "@astrojs/sitemap": "3.2.1",
    "@astrojs/vercel": "8.1.1",
    "@midudev/tailwind-animations": "0.0.7",
    "@types/dom-view-transitions": "1.0.4",
    "@typescript-eslint/parser": "7.15.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "1.2.2",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "postcss-import": "16.1.0",
    "postcss-nesting": "12.1.5",
    "prettier": "3.3.2",
    "prettier-plugin-astro": "0.14.0",
    "prettier-plugin-astro-organize-imports": "0.4.9",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "4.0.11",
    "typescript": "5.5.3",
    "vite-plugin-pwa": "0.20.0"
  }
